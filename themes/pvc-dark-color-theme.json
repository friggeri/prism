    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#9966ffff"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#ff8000ff"
      }
    }
