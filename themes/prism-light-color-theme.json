  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#798286ff"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#242728ff"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#b39500ff"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#ff006aff"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#242728ff"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#7733ffff"
      }
    },
    {
